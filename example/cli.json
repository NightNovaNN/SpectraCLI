{
    "name": "SpaceTool",
    "banner": true,
    "banner_color": "cyan",
    "commands": {
        "launch": "s=Spinner('Launching rocket','yellow');s.start();time.sleep(2);s.stop('Rocket launched!')",
        "progress": "progress(30,0.03,'green')",
